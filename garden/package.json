  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "npm run build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy-staging": "gh-pages -d build -r https://github.com/Garden-AI/garden-frontend-staging",
    "deploy-prod": "gh-pages -d build",
    "remove-mock-service-worker": "sh -c \"rm -f build/mockServiceWorker.js\"",
    "test": "vitest",
    "refresh-types": "curl -s https://api-dev.thegardens.ai/openapi.json | npx openapi-typescript -o ./src/api/backend-schema.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "@tanstack/eslint-plugin-query": "^5.43.1",
    "@types/node": "^20.14.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.19",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "msw": "^2.3.1",
    "openapi-typescript": "^7.0.0",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,css,md}": "prettier --write"
  }
}
