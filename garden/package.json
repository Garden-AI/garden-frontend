{
  "name": "garden",
  "version": "0.1.0",
  "homepage": ".",
  "type": "module",
  "private": true,
  "dependencies": {
    "@globus/sdk": "3.3.1",
    "@globus/types": "^0.0.7",
    "@szhsin/react-accordion": "^1.2.4",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.13.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.0",
    "marked-react": "^2.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-router-dom": "^6.23.1",
    "react-syntax-highlighter": "^15.5.0",
    "vite": "^5.2.12",
    "vite-tsconfig-paths": "^4.3.2",
    "web-vitals": "^4.0.1"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "build:production": "npm run build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy-staging": "gh-pages -d build -r https://github.com/Garden-AI/garden-frontend-staging",
    "deploy-prod": "gh-pages -d build",
    "remove-mock-service-worker": "sh -c \"rm -f build/mockServiceWorker.js\"",
    "test": "vitest"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.19",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "msw": "^2.3.0",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.4",
    "@tailwindcss/typography": "^0.5.13",
    "vitest": "^1.6.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,css,md}": "prettier --write"
  }
}
